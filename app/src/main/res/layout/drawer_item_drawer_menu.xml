<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="drawerItemDrawerMenuModel" type="com.android.app.modules.itemdrawermenu.data.model.DrawerItemDrawerMenuModel"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearGroup149"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="match_parent"
            android:layout_weight="1" android:layout_width="match_parent">
            <FrameLayout android:id="@+id/frameGroup147"
                android:layout_height="@dimen/_769pxv" android:layout_width="@dimen/_287pxh">
                <View android:id="@+id/viewBackground"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_769pxh" android:layout_width="@dimen/_287pxh"/>
                <LinearLayout android:id="@+id/linearGroup19"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_A700">
                    <LinearLayout android:gravity="center_horizontal"
                        android:id="@+id/linearGroup144"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearGroup143"
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <ImageView
                                android:id="@+id/imageProfilePicture"
                                android:layout_height="@dimen/_62pxh"
                                android:layout_marginStart="@dimen/_28pxh"
                                android:layout_width="@dimen/_59pxh"
                                android:scaleType="fitXY"
                                imageRes="@{@drawable/img_profilepicture_1}" tools:src="@drawable/img_profilepicture_1"/>
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearGroup8"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_8pxv"
                                android:layout_marginEnd="@dimen/_96pxh"
                                android:layout_marginStart="@dimen/_16pxh"
                                android:layout_marginTop="@dimen/_7pxv"
                                android:layout_weight="0.59"
                                android:layout_width="0dp" android:orientation="vertical">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtWelcome"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="@dimen/_10pxh"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{drawerItemDrawerMenuModel.txtWelcome}"
                                    style="@style/txtPoppinsregular14_4" tools:text="@string/lbl_welcome"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtJohnDoe"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="@dimen/_5pxv"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{drawerItemDrawerMenuModel.txtJohnDoe}"
                                    style="@style/txtPoppinsmedium18_1" tools:text="@string/lbl_john_doe"/>
                            </LinearLayout>
                        </LinearLayout>
                        <View android:background="@color/black_900"
                            android:id="@+id/lineLine5"
                            android:layout_gravity="center_horizontal"
                            android:layout_height="@dimen/_1pxv"
                            android:layout_marginEnd="@dimen/_27pxh"
                            android:layout_marginStart="@dimen/_28pxh"
                            android:layout_marginTop="@dimen/_34pxv" android:layout_width="@dimen/_232pxh"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearDrawerOptions"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_28pxh"
                        android:layout_marginStart="@dimen/_28pxh"
                        android:layout_marginTop="@dimen/_69pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <TextView android:clickable="true"
                            android:ellipsize="end"
                            android:id="@+id/txtHome"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{drawerItemDrawerMenuModel.txtHome}"
                            style="@style/txtPoppinsmedium18_1" tools:text="@string/lbl_home"/>
                        <TextView android:clickable="true"
                            android:ellipsize="end"
                            android:id="@+id/txtExplore"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_29pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{drawerItemDrawerMenuModel.txtExplore}"
                            style="@style/txtPoppinsmedium18_1" tools:text="@string/lbl_explore"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtInterests"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_29pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{drawerItemDrawerMenuModel.txtInterests}"
                            style="@style/txtPoppinsmedium18_1" tools:text="@string/lbl_interests"/>
                        <TextView android:clickable="true"
                            android:ellipsize="end"
                            android:id="@+id/txtTermsAndCondi"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_29pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{drawerItemDrawerMenuModel.txtTermsAndCondi}"
                            style="@style/txtPoppinsmedium18_1" tools:text="@string/msg_terms_and_condi2"/>
                        <TextView android:clickable="true"
                            android:ellipsize="end"
                            android:id="@+id/txtPrivacyPolicy"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_29pxv"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{drawerItemDrawerMenuModel.txtPrivacyPolicy}"
                            style="@style/txtPoppinsmedium18_1" tools:text="@string/lbl_privacy_policy"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearGroup10"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_37pxv"
                        android:layout_marginEnd="@dimen/_27pxh"
                        android:layout_marginStart="@dimen/_28pxh"
                        android:layout_marginTop="@dimen/_282pxv"
                        android:layout_width="match_parent" android:orientation="vertical">
                        <View android:background="@color/black_900"
                            android:id="@+id/lineLine6"
                            android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_232pxh"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLogout"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_1pxh"
                            android:layout_marginTop="@dimen/_27pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{drawerItemDrawerMenuModel.txtLogout}"
                            style="@style/txtPoppinsmedium18" tools:text="@string/lbl_logout"/>
                    </LinearLayout>
                </LinearLayout>
            </FrameLayout>
        </ScrollView>
    </LinearLayout>
</layout>
