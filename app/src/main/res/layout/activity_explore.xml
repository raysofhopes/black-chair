<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="exploreVM" type="com.android.app.modules.explore.data.viewmodel.ExploreVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearExplore"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearGroup77"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginTop="@dimen/_28pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearGroup73"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="@dimen/_28pxh"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <LinearLayout android:id="@+id/linearGroup80"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.69"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageDrawericon"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_8pxv"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="@dimen/_19pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_drawericon"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtExplore"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_22pxh"
                            android:layout_weight="0.83"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{exploreVM.exploreModel.txtExplore}"
                            style="@style/txtPoppinssemibold24" tools:text="@string/lbl_explore"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearGroup81"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginStart="@dimen/_123pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_weight="0.31"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageIconNotificati"
                            android:layout_height="@dimen/_21pxh"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_notificationic"/>
                        <ImageView android:id="@+id/imageIconSearch"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY" android:src="@drawable/img_iconsearch"/>
                    </LinearLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <EditText
                android:background="@drawable/rectangle_bg_black_900_63_radius_10"
                android:drawablePadding="@dimen/_10pxh"
                android:drawableStart="@drawable/img_search_icon_1"
                android:ellipsize="end"
                android:hint="@string/lbl_search"
                android:id="@+id/etSearch"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_28pxh"
                android:layout_marginStart="@dimen/_28pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_18pxv"
                android:paddingStart="@dimen/_20pxh"
                android:paddingTop="@dimen/_18pxv"
                android:singleLine="true"
                android:text="@={exploreVM.exploreModel.etSearchValue}"
                android:textColorHint="@color/gray_700" style="@style/etSolidRounded"/>
            <LinearLayout android:gravity="end"
                android:id="@+id/linearGroup66"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnScience"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_width="@dimen/_83pxh"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_10pxh"
                    android:paddingStart="@dimen/_11pxh"
                    android:paddingTop="@dimen/_9pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_science"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_science"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnLoremIpsum"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_width="@dimen/_125pxh"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_15pxh"
                    android:paddingStart="@dimen/_15pxh"
                    android:paddingTop="@dimen/_9pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_lorem_ipsum"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_lorem_ipsum"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnDesign"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_width="@dimen/_83pxh"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_13pxh"
                    android:paddingStart="@dimen/_13pxh"
                    android:paddingTop="@dimen/_9pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_design"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_design"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnTechnology"
                    android:layout_height="@dimen/_40pxv"
                    android:layout_marginStart="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_108pxh"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_13pxh"
                    android:paddingStart="@dimen/_13pxh"
                    android:paddingTop="@dimen/_9pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_technology"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded" tools:text="@string/lbl_technology"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtRECOMMENDEDFOR"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_54pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{exploreVM.exploreModel.txtRECOMMENDEDFOR}"
                style="@style/txtPoppinssemibold14" tools:text="@string/msg_recommended_for"/>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerExplore"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_20pxh}" tools:itemCount="3" tools:listitem="@layout/row_explore1"/>
        </LinearLayout>
    </LinearLayout>
</layout>
